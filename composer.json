{
    "name": "somnambulist/fractal-bundle",
    "type": "symfony-bundle",
    "description": "A port of samj/fractal-bundle for integrating TheLeague Fractal into Symfony",
    "keywords": [
        "fractal",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "info@somnambulist.tech"
        }
    ],
    "require": {
        "php": ">=8.1",
        "league/fractal": "^0.20",
        "symfony/framework-bundle": "^6.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "^7.0",
        "symfony/var-dumper": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\Bundles\\FractalBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Bundles\\FractalBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
